module Lobby

entity Lobby ={
inherit PlayerLobby

Players : int

rule Players =
    wait 2.0f
    Players + 1    

Create()={
    Players = 0
}

}

entity Host ={

    StartGame : bool
    PlayersConnected : bool

        

    rule GameReady = 
        wait StartGame
        Application.LoadLevel("Contact")

Create()={
    StartGame = false
    PlayersConnected = false
}

}
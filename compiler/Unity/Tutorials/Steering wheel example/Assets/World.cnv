module Game

open "UnityEngine.dll"
open UnityEngine

worldEntity World = {

  Blokje : Cube



  Create() = {
  Blokje = new Cube()
  }
}

entity Cube = {
  inherit UnityCube

  rule Rotation =
    let rot = new Vector3(0.0f, Steering * 180.0f, 0.0f)
    yield rot

  Create() = {
  Base = UnityCube.Find()
  }
}